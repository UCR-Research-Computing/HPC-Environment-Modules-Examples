#%Module1.0#####################################################################
##
## anaconda modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tmemcached - Adds memcached and related packages to your path"
}

module-whatis	"memcached-1.6.9"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH            ${hpcc_modules}/memcached/1.6.9/bin
prepend-path    CPATH           ${hpcc_modules}/memcached/1.6.9/include
prepend-path    MANPATH         ${hpcc_modules}/memcached/1.6.9/share/man
