proc ModulesHelp { } {
        global version prefix

        puts stderr "\tminiasm - Adds miniasm to your path"
}

module-whatis	"miniasm-0.3"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/miniasm/0.3/bin
prepend-path	MANPATH		${hpcc_modules}/miniasm/0.3/share/man

