#%Module1.0#####################################################################
##
## ASTRAL_pro modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Sets ASTRAL-Pro JAR path to your environment"
}

module-whatis   "ASTRAL-Pro-1.1.3"

set      hpcc_modules    $::env(HPCC_MODULES)

module load java
setenv          ASTRALDIR	${hpcc_modules}/ASTRAL-Pro/1.1.3/astral.1.1.3.jar
setenv          ASTRALJAR	${hpcc_modules}/ASTRAL-Pro/1.1.3/astral.1.1.3.jar

