#%Module1.0#####################################################################
##
## texlive modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\ttexlive - Adds texlive to your path"
}

module-whatis	"texlive-20220403"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/texlive/20220403/bin/x86_64-linux

