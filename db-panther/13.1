#%Module1.0#####################################################################
##
## Panther database modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\db-panther - Sets Panther database path to PANTHER_DB environment variable"
}

module-whatis	"db-panther"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

setenv      PANTHER_DB	    /srv/projects/db/panther/13.1

module      use    ${modules_dir}

