#%Module1.0#####################################################################
##
## mathematica modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tmathematica - Adds mathematica to your path"
}

module-whatis	"mathematica-12.3.1"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH                     ${hpcc_modules}/mathematica/12.3.1/Executables
prepend-path    LD_LIBRARY_PATH          ${hpcc_modules}/mathematica/12.3.1/SystemFiles/Libraries/Linux-x86-64
setenv          MATHEMATICA_BASE         ${hpcc_modules}/mathematica/12.3.1
setenv          MATHEMATICA_USERBASE     ~/.Mathematica

