#%Module1.0#####################################################################
##
## miniprot modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tminiprot - Adds miniprot to your path"
}

module-whatis	"miniprot-0.2"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/miniprot/0.2/bin
prepend-path	MANPATH		${hpcc_modules}/miniprot/0.2/share/man

