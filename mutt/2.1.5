#%Module1.0#####################################################################
##
## mutt modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tmutt - Adds mutt to your path"
}

module-whatis	"mutt-2.1.5"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH        ${hpcc_modules}/mutt/2.1.5/bin
prepend-path	MANPATH		${hpcc_modules}/mutt/2.1.5/share/man


