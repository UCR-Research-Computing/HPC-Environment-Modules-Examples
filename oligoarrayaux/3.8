#%Module1.0#####################################################################
##
## OligoArrayAux modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tOligoArrayAux - Adds OligoArrayAux to your path"
}

module-whatis	"OligoArrayAux-3.8"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH            ${hpcc_modules}/oligoarrayaux/3.8/bin/

