#%Module1.0#####################################################################
##
## ASTRAL modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Sets ASTRAL JAR path to your environment"
}

module-whatis   "ASTRAL-4.10.7"

set      hpcc_modules    $::env(HPCC_MODULES)

module load java
setenv          ASTRALDIR	${hpcc_modules}/ASTRAL/4.10.7
setenv          ASTRALJAR	${hpcc_modules}/ASTRAL/4.10.7/astral.jar

