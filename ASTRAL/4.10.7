#%Module1.0#####################################################################
##
## ASTRAL modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Sets ASTRL JAR path to your environment"
}

module-whatis   "ASTRAL-4.10.7"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set     version         3.2.9
set     prefix          ${modules_dir}/${version}
set     exec_prefix     ${prefix}
set     datarootdir     ${prefix}/share

module load java
setenv          ASTRALDIR	${iigb_modules}/ASTRAL/4.10.7
setenv          ASTRALJAR	${iigb_modules}/ASTRAL/4.10.7/astral.jar

module          use            ${modules_dir}

