#%Module1.0#####################################################################
##
## GATK modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgatk - Adds gatk to your path"
}

module-whatis	"gatk-3.7"

set      hpcc_modules    $::env(HPCC_MODULES)

module load java
setenv  GATKDIR     ${hpcc_modules}/gatk/3.7
setenv  GATK        ${hpcc_modules}/gatk/3.7/GenomeAnalysisTK.jar 

