#%Module1.0#####################################################################
##
## GATK modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgatk - Adds gatk to your path"
}

module-whatis	"gatk-4.0.1.2"

set      hpcc_modules    $::env(HPCC_MODULES)

module load java

prepend-path    PATH        ${hpcc_modules}/gatk/4.0.1.2

setenv          GATKDIR     ${hpcc_modules}/gatk/4.0.1.2

