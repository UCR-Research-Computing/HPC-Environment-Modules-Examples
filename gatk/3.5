#%Module1.0#####################################################################
##
## maui modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgatk - Adds gatk to your path"
}

module-whatis	"gatk-3.5"

set      hpcc_modules    $::env(HPCC_MODULES)

module load java 

setenv  GATKDIR     ${hpcc_modules}/gatk/3.5
setenv  GATK        ${hpcc_modules}/gatk/3.5/GenomeAnalysisTK.jar 

