#%Module1.0#####################################################################
##
## GATK modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgatk - Adds gatk to your path"
}

module-whatis	"gatk-4.1.8.1"

set      hpcc_modules    $::env(HPCC_MODULES)

module load java

setenv  GATKDIR     ${hpcc_modules}/gatk/4.1.8.1
setenv  GATK        ${hpcc_modules}/gatk/4.1.8.1/gatk-package-4.1.8.1-local.jar

prepend-path    PATH        ${hpcc_modules}/gatk/4.1.8.1

setenv          GATKDIR     ${hpcc_modules}/gatk/4.1.8.1

