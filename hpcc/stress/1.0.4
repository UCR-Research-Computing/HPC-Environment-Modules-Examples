#%Module1.0#####################################################################
##
## stress modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tstress- Adds stress to your path"
}

module-whatis	"stress-1.0.4"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH            ${iigb_modules}/hpcc/stress/1.0.4/bin
prepend-path    MANPATH         ${iigb_modules}/hpcc/stress/1.0.4/share/man

