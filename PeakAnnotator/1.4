#%Module1.0#####################################################################
##
## PeakAnnotator modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Adds PeakAnnotator to your path"
}

module-whatis   "PeakAnnotator-1.4"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set     version         3.2.9
set     prefix          ${modules_dir}/${version}
set     exec_prefix     ${prefix}
set     datarootdir     ${prefix}/share

prepend-path    PATH               ${hpcc_modules}/PeakAnnotator/1.4/


