#%Module1.0#####################################################################
##
## MCScanX modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tMCScanX - Adds MCScanX to your path"
}

module-whatis   "MCScanX-r51_g97e74f4"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH             ${hpcc_modules}/MCScanX/r51_g97e74f4
