#%Module1.0#####################################################################
##
## LinkImpute modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Sets LinkImpute to your environment"
}

module-whatis   "LinkImpute-1.1.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set     version         3.2.9
set     prefix          ${modules_dir}/${version}
set     exec_prefix     ${prefix}
set     datarootdir     ${prefix}/share

module load java

prepend-path    PATH           ${hpcc_modules}/linkimpute/1.1.1/bin
prepend-path    CLASSPATH      ${hpcc_modules}/linkimpute/1.1.1/lib


