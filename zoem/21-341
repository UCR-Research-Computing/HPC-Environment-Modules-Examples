#%Module1.0#####################################################################
##
## zoem modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tzoem - Adds zoem to your path"
}

module-whatis	"zoem-21-341"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/zoem/21-341/bin
prepend-path    MANPATH         ${hpcc_modules}/zoem/21-341/share/man

