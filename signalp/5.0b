#%Module1.0#####################################################################
##
## signalp modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tsignalp - Adds signalp to your path"
}

module-whatis	"signalp-5.0b"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/signalp/5.0b/bin
prepend-path    MANPATH         ${hpcc_modules}/signalp/5.0b/share/man

