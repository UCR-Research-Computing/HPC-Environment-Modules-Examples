#%Module1.0#####################################################################
##
## harvest-tools modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tharvest-tools - Adds parsnp,gingr,harvesttools to your path"
}

module-whatis	"harvest-tools-1.2"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		${iigb_modules}/harvest-tools/1.2/bin

module		use		${modules_dir}

