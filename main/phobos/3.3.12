#%Module1.0#####################################################################
##
## phobos modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tphobos - Adds phobos to your path"
}

module-whatis	"phobos-3.3.12"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/phobos/3.3.12/bin
module		use		/usr/local/Modules/versions
