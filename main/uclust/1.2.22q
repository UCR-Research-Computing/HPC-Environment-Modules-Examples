#%Module1.0#####################################################################
##
## uclust modulefile
##
proc ModulesHelp { } {
        global version prefix

       puts stderr "\tuclust- Adds uclust to your path"
}

module-whatis	"uclust-1.2.22q"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/uclust/1.2.22q/bin
module		use		/usr/local/Modules/versions
