#%Module1.0#####################################################################
##
## trinotate modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\ttrinotate - Adds trinotate RNA-Seq annotation to your path"
}

module-whatis	"trinotate-r20131110"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

module load java
module load hmmer
module load rnammer
module load signalp/4.1
module load tmhmm
module load ncbi-blast
setenv TRINOTATE_HOME	/opt/trinotate/r20131110
prepend-path	PATH	${TRINOTATE_HOME}
prepend-path	PATH	${TRINOTATE_HOME}/util
prepend-path	CLASSPATH	${TRINOTATE_HOME}/java
preped-path	PERL5LIB	${TRINOTATE_HOME}/PerlLib
module		use		/usr/local/Modules/versions
