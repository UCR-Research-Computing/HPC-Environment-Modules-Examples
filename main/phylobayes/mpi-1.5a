#%Module1.0#####################################################################
##
## phylobayes modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tphylobayes - Adds phylobayes tools to your path"
}

module-whatis	"phylobayes-mpi-1.5a"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		/opt/phylobayes/mpi-1.5a/bin
module		use		/usr/local/Modules/versions

