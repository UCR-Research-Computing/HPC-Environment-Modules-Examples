#%Module1.0#####################################################################
##
## fermi modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tfermi - Adds fermi assembler in your path"
}

module-whatis	"fermi-r706"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/fermi/r706/bin
module		use		/usr/local/Modules/versions
