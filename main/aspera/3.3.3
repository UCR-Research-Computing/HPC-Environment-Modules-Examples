#%Module1.0#####################################################################
##
## aspera modulefile
##
proc ModulesHelp { } {
        global version prefix

       puts stderr "\taspera- Adds aspera plugin tools to your path"
}

module-whatis	"aspera-3.3.3"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/aspera/3.3.3/bin
setenv	ASPERAKEY	/opt/aspera/3.3.3/etc/asperaweb_id_dsa.putty
module		use		/usr/local/Modules/versions

