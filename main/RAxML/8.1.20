#%Module1.0#####################################################################
##
## RAxML modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tRAxML - Adds RAxML 8.1.20 your path"
}

module-whatis	"RAxML-8.1.20"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		/opt/RAxML/8.1.20/bin

module		use		/usr/local/Modules/versions

