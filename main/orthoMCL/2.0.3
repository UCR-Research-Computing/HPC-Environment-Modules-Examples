#%Module1.0#####################################################################
##
## OrthoMCL modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tOrthoMCL - Adds OrthoMCL to your path"
}

module-whatis	"orthomcl-2.0.3"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		/opt/orthomcl/2.0.3/bin

module		use		/usr/local/Modules/versions

