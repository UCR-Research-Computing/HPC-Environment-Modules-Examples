#%Module1.0#####################################################################
##
## qiime modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tqiime- Adds qiime and scripts to your path"
}

module-whatis	"qiime-1.5.0-dev"
module-verbosity yes

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/qiime/1.5.0-dev/bin
module		use		/usr/local/Modules/versions
setenv RDP_JAR_PATH	/opt/qiime/1.5.0-dev/app/rdp_classifier.jar
prepend-path	PYTHONPATH	/opt/qiime/1.5.0-dev/lib

module load java AmpliconNoise/1.25 cd-hit/3.1.1 cdbfasta/0.99 mothur microbiomeutil clearcut \
 infernal/1.0.2 RAxML/7.3.2 pplacer ParsInsert rtax/0.982 muscle/3.8.31 uclust usearch \
 FastTree/2.1.3 stajichlab stajichlab-python ncbi-blast/2.2.22

