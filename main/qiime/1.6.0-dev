#%Module1.0#####################################################################
##
## qiime modulefile
##
module-log debug stdout
module-log verb stdout
module-trace on

proc ModulesHelp { } {
        global version prefix

        puts stderr "\tqiime- Adds qiime and scripts to your path"
}

module-whatis	"qiime-1.6.0-dev"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

module          use             /usr/local/Modules/versions

prepend-path    PATH    /opt/qiime/1.6.0-dev/bin
setenv RDP_JAR_PATH     /opt/RDP/Classifiers/rdp_classifier_2.5/rdp_classifier-2.5.jar
prepend-path    PYTHONPATH      /opt/qiime/1.6.0-dev/lib

