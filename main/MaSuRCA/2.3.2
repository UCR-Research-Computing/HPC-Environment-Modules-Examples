prepend-path	PATH	/opt/MaSuRCA/2.3.2/CA/Linux-amd64/bin
prepend-path	PATH	/opt/MaSuRCA/2.3.2/bin
prepend-path	LD_LIBRARY_PATH	/opt/MaSuRCA/2.3.2/lib
prepend-path	MANPATH	/opt/MaSuRCA/2.3.2/share/man

module		use		/usr/local/Modules/versions
