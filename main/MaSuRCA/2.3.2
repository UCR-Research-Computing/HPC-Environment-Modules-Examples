proc ModulesHelp { } {
        global version prefix

        puts stderr "\tMaSuRCA- Adds MaSuRCA to your path"
}

module-whatis	"MaSuRCA-2.3.2"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
