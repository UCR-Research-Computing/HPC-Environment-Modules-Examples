#%Module1.0#####################################################################
##
## mothur modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tmothur - Adds mothur software your path"
}

module-whatis	"mothur-1.25.0"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/mothur/1.25.0/bin
module		use		/usr/local/Modules/versions
