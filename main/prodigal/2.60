#%Module1.0#####################################################################
##
## prodigal modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tprodigal - Adds prodigal to your path"
}

module-whatis	"prodigal 2.60"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/prodigal/2.60/bin
module		use		/usr/local/Modules/versions
