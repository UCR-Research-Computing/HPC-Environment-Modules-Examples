#%Module1.0#####################################################################
##
## hydra modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\thydra - Adds hydra SV tool to your path"
}

module-whatis	"hydra-0.5.3"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/hydra/0.5.3/bin
prepend-path	PATH	/opt/hydra/0.5.3/scripts
module		use		/usr/local/Modules/versions
