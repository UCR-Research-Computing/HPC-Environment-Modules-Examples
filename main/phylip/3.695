#%Module1.0#####################################################################
##
## PHYLIP modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tPHYLIP- Adds phylip phylogenetics exes to your path"
}

module-whatis	"phylip-3.695"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

append-path	PATH	/opt/phylip/3.695/bin
module		use		/usr/local/Modules/versions
