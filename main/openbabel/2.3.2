#%Module1.0#####################################################################
##
## JAVA JDK modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tOpen Babel- Adds OpenBabel 2.3.2 to your path"
}

module-whatis	"openbabel-2.3.2"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prereq           boost/1.51.0
prepend-path     CPATH             /opt/openbabel/2.3.2/include
prepend-path     LIBRARY_PATH      /opt/openbabel/2.3.2/lib
prepend-path     PATH              /opt/openbabel/2.3.2/bin
prepend-path     LD_LIBRARY_PATH   /opt/openbabel/2.3.2/lib
prepend-path     MANPATH           /opt/openbabel/2.3.2/share/man

module           use      /usr/local/Modules/versions
