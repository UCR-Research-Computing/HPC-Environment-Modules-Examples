#%Module1.0#####################################################################
##
## OMA modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tOMA- Adds oma and related exes to your path"
}

module-whatis	"OMA-0.99x"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/OMA/0.99x/bin
module		use		/usr/local/Modules/versions
