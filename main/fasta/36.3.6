#%Module1.0#####################################################################
##
## fasta36 modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tFASTA36 - Adds FASTA36 and applications to your path"
}

module-whatis	"fasta-36.3.6"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/fasta/36.3.6/bin
prepend-path	MANPATH	/opt/fasta/36.3.6/share/man
module		use		/usr/local/Modules/versions
