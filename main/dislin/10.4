#%Module1.0#####################################################################
##
## dislin modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tdislin - Adds dislin to your library path"
}

module-whatis	"dislin-10.4"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    LD_LIBRARY_PATH    /opt/dislin/10.4

module		use		/usr/local/Modules/versions

