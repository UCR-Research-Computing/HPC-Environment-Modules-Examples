#%Module1.0#####################################################################
##
## usearch modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tusearch - Adds usearc to your path"
}

module-whatis	"usearch-5.2.32"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/usearch/5.2.32/bin
module		use		/usr/local/Modules/versions
