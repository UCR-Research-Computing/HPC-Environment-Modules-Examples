#%Module1.0#####################################################################
##
## eXpress modulefile
##
proc ModulesHelp { } {
        global version prefix

       puts stderr "\teXpress - Adds eXpress RNA-Seq processing to your path"
}

module-whatis	"eXpress-1.0.0"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/eXpress/1.0.0
module		use		/usr/local/Modules/versions
