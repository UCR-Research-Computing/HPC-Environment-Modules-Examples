#%Module1.0#####################################################################
##
## JAVA JDK modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tJava- Adds java 7 to your path"
}

module-whatis	"oracle-jdk1.7.0_17"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path     PATH     /opt/java/jdk1.7.0_17/bin
prepend-path     MANPATH  /opt/java/jdk1.7.0_17/man
prepend-path     CPATH    /opt/java/jdk1.7.0_17/include
prepend-path     LIBRARY_PATH /opt/java/jdk1.7.0_17/lib

module           use      /usr/local/Modules/versions
