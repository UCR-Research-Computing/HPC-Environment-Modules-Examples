#%Module1.0#####################################################################
##
## clearcut modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tclearcut- Adds clearcut to your path"
}

module-whatis	"clearcut-1.0.9"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/clearcut/1.0.9/bin
module		use		/usr/local/Modules/versions
