#%Module1.0#####################################################################
##
## Structure modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tstructure - Adds structure 2.3.4 to your path"
}

module-whatis	"structure-2.3.4"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

puts stderr "Structure requires  mainparams and extraparams in your working directory in order to function.\nYou may copy the generic templates from /opt/structure/2.3.4 or create your own."
prepend-path    PATH   		/opt/structure/2.3.4

module		use		/usr/local/Modules/versions

