#%Module1.0#####################################################################
##
## Structure modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tstructure - Adds structure 2.3.4 to your path"
}

module-whatis	"structure-2.3.4"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

puts stdout "In order to use structure, you must copy mainparams and extraparams from /opt/structure/2.3.4 to your working directory and set your parameters accordingly."
prepend-path    PATH   		/opt/structure/2.3.4

module		use		/usr/local/Modules/versions

