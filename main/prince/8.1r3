#%Module1.0#####################################################################
##
## prince modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tprince - Adds prince XML to PDF in your path"
}

module-whatis	"prince-8.1r3"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/prince/8.1r3
module		use		/usr/local/Modules/versions
