#%Module1.0#####################################################################
##
## pplacer modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tpplacer - Adds pplacer software your path"
}

module-whatis	"pplacer-1.1.alpha13"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/pplacer/1.1.alpha13/bin
module		use		/usr/local/Modules/versions
