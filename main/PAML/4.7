#%Module1.0#####################################################################
##
## PAML modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tPAML- Adds codeml, baseml and friends to your path"
}

module-whatis	"PAML-4.7"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/PAML/4.7/bin
module		use		/usr/local/Modules/versions
