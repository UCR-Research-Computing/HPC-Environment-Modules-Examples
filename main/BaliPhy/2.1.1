#%Module1.0#####################################################################
##
## BaliPhy modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tBaliPhy - Adds BaliPhy tool in your path"
}

module-whatis	"BaliPhy-2.1.1"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/BaliPhy/2.1.1/bin
module		use		/usr/local/Modules/versions
