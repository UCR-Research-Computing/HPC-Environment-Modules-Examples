#%Module1.0#####################################################################
##
## tbl2asn modulefile
## 
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\ttbl2asn - Adds tbl2asn to your path"
}

module-whatis	"tbl2asn-2015-03-23"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/tbl2asn/2015-03-23/bin
module		use		/usr/local/Modules/versions
