#%Module1.0#####################################################################
##
## Python3 modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tpython3 - Adds python3 to your path"
}

module-whatis	"python-3.2.2"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		 /opt/gcc/4.7.2/bin
prepend-path    MANPATH  	 /opt/gcc/4.7.2/share/man
prepend-path    CPATH        /opt/gcc/4.7.2/include
prepend-path    LIBRARY_PATH /opt/gcc/4.7.2/lib:/opt/gcc/4.7.2/lib64
prepend-path    LD_LIBRARY_PATH /opt/gcc/4.7.2/lib:/opt/gcc/4.7.2/lib64

module		use		/usr/local/Modules/versions

