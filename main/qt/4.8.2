#%Module1.0#####################################################################
##
## QT modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tQT - Adds QT 4.8.2 to your path"
}

module-whatis	"libqt-4.8.2"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		/opt/qt/4.8.2/bin
prepend-path    CPATH       /opt/qt/4.8.2/include
prepend-path    LIBRARY_PATH /opt/qt/4.8.2/lib

module		use		/usr/local/Modules/versions

