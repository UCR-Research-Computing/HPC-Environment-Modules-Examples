#%Module1.0#####################################################################
##
## JAVA JDK modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tSEQuel- Adds SEQuel 1.0.2 to your path"
}

module-whatis	"SEQuel-1.0.2"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

module load bwa/0.7.4
prereq           bwa/0.7.4

prepend-path     PATH     /opt/SEQuel/1.0.2

module           use      /usr/local/Modules/versions
