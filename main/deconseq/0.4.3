#%Module1.0#####################################################################
##
## deconseq modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tdeconseq - Adds deconseq to your path"
}

module-whatis	"deconseq-0.4.1"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PERL5LIB /opt/deconseq/0.4.3/lib
prepend-path    PATH     /opt/deconseq/0.4.3/bin

module          use      /usr/local/Modules/versions
