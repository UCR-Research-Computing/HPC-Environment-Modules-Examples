proc ModulesHelp { } {
        global version prefix

       puts stderr "\tdetonate - Adds rsem- and ref-eval RNA-Seq assembly evaluation tools to your path"
}

module-whatis	"detonate-1.8.1 - rsem- and ref-eval RNA-Seq assembly evaluation tools"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/detonate/1.8.1/bin
module		use		/usr/local/Modules/versions
