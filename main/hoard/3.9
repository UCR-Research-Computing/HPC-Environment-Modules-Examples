#%Module1.0#####################################################################
##
## libhoard modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tlibhoard - Adds libhoard memory allocator library in your path"
}

module-whatis	"hoard-3.9"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	LD_PRELOAD	/opt/hoard/3.9/lib
module		use		/usr/local/Modules/versions
