#%Module1.0#####################################################################
##
## ChloroP modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tChloroP - Adds ChloroP 1.1 to your path"
}

module-whatis	"chlorop-1.1"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		/opt/chlorop/1.1
prepend-path    MANPATH  	/opt/chlorop/1.1

module		use		/usr/local/Modules/versions

