#%Module1.0#####################################################################
##
## R modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Adds R to your path"
}

module-whatis	"R-2.15.1"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		 /opt/R-2.10.1/bin
prepend-path	MANPATH		 /opt/R-2.10.1/share/man
prepend-path    CPATH        /opt/R-2.10.1/lib64/R/include
prepend-path    LIBRARY_PATH /opt/R-2.10.1/lib64/R/lib

module		use		/usr/local/Modules/versions

