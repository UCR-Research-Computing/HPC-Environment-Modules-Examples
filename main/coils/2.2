#%Module1.0#####################################################################
##
## coils modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tcoils - Adds coils to your path"
}

module-whatis	"coils-2.2"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/coils/2.2
setenv	COILSDIR	/opt/coils/2.2
module		use		/usr/local/Modules/versions
