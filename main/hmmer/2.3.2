#%Module1.0#####################################################################
##
## hmmer modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\thmmer - Adds hmmer2 to your path"
}

module-whatis	"hmmer-2.3.2"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		/opt/hmmer/2.3.2/bin
prepend-path    MANPATH  	/opt/hmmer/2.3.2/man

module		use		/usr/local/Modules/versions

