#%Module1.0#####################################################################
##
## Prokka modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tProkka - Adds prokka and related exes to your path"
}

module-whatis	"prokka 1.7"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/Prokka/1.7/bin
module		use		/usr/local/Modules/versions
