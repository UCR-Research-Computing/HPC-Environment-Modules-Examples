#%Module1.0#####################################################################
##
## BEAST modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tBEAST - Adds BEAST to your path"
}

module-whatis	"BEAST-2.1.2"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/BEAST/2.1.2/bin
prepend-path	LD_LIBRARY_PATH /opt/BEAST/2.1.2/lib
module		use		/usr/local/Modules/versions
