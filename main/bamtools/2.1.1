#%Module1.0#####################################################################
##
## bamtools modulefile
##
proc ModulesHelp { } {
        global version prefix

       puts stderr "\tbamtools- Adds bamtools to your path"
}

module-whatis	"bamtools-2.1.1"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/bamtools/2.1.1/bin
prepend-path	LD_LIBRARY_PATH	/opt/bamtools/2.1.1/lib
module		use		/usr/local/Modules/versions
