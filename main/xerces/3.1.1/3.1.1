#%Module1.0#####################################################################
##
## samtools modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\txerces - Adds xerces to your path"
}

module-whatis	"xerces-3.1.1"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH         /opt/xerces/3.1.1/bin
prepend-path    LIBRARY_PATH /opt/xerces/3.1.1/lib
prepend-path    CPATH        /opt/xerces/3.1.1/include
prepend-path    MANPATH      /opt/xerces/3.1.1/share/man


module		use		/usr/local/Modules/versions


