#%Module1.0#####################################################################
##
## cpc modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tcpc- Adds cpc 0.9 to your path"
}

module-whatis	"cpc-0.9"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path     LIBRARY_PATH      /opt/cpc/0.9/lib
prepend-path     PATH              /opt/cpc/0.9/bin

module           use      /usr/local/Modules/versions
