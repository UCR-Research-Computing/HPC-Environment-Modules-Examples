#%Module1.0#####################################################################
##
## JAVA JDK modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tSherman - Adds Sherman 0.1.5 to your path"
}

module-whatis	"sherman-0.1.5"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path     PATH              /opt/sherman/0.1.5

module           use      /usr/local/Modules/versions
