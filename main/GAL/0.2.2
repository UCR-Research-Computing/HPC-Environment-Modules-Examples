#%Module1.0#####################################################################
##
## GAL modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tGAL - Adds GAL to your path"
}

module-whatis	"vim-7.4"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

module load perl/5.16.3
prepend-path    PATH   		/opt/GAL/0.2.2/bin
prepend-path    PERL5LIB	/opt/GAL/0.2.2/lib
module		use		/usr/local/Modules/versions

