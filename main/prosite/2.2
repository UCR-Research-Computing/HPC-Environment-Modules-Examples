#%Module1.0#####################################################################
##
## prosite modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tprosite- Adds prosite tools to your path"
}

module-whatis	"prosite-2.2"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/prosite/2.2/bin
prepend-path	MANPATH	/opt/prosite/2.2/share/man
module		use		/usr/local/Modules/versions
