#%Module1.0#####################################################################
##
## agp_validate modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tagp_validate - Adds NCBI agp_validate to your path"
}

module-whatis	"agp_validate-2012-04-25"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		/opt/agp_validate/2012-04-25/bin
module		use		/usr/local/Modules/versions

