#%Module1.0#####################################################################
##
## genometools modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgenometools - Adds genometools your path"
}

module-whatis	"genometools-1.4.1"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH   		/opt/genometools/1.4.1/bin
prepend-path	LD_LIBRARY_PATH	/opt/genometools/1.4.1/lib
module		use		/usr/local/Modules/versions

