#%Module1.0#####################################################################
##
## LAST modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tLAST - Adds LAST env variables and exes to your path"
}

module-whatis	"LAST-362"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/last/362/bin
module		use		/usr/local/Modules/versions
