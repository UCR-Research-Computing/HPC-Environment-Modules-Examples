#%Module1.0#####################################################################
##
## GenomeMapper modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tGenomeMapper - Adds GenomeMapper 0.4.4 to your path"
}

module-whatis	"genomemapper-0.4.4"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		/opt/genomemapper/0.4.4

module		use		/usr/local/Modules/versions

