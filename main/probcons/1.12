#%Module1.0#####################################################################
##
## probcons modulefile
##
proc ModulesHelp { } {
        global version prefix

       puts stderr "\tprobcons- Adds probcons your path"
}

module-whatis	"probcons-1.12"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/probcons/1.12/bin
module		use		/usr/local/Modules/versions
