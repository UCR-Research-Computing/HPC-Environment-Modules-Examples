#%Module1.0#####################################################################
##
## Ruby 1.9.3 modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\truby - Adds ruby to your path"
}

module-whatis	"ruby-1.9.3"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		/opt/ruby/1.9.3/bin
prepend-path    MANPATH  	/opt/ruby/1.9.3/share/man

module		use		/usr/local/Modules/versions

