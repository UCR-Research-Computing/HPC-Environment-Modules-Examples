#%Module1.0#####################################################################
##
## icommands modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\ticommands - Adds icommands to your path"
}

module-whatis	"icommands-iPlant"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/icommands/iPlant

module		use		/usr/local/Modules/versions
