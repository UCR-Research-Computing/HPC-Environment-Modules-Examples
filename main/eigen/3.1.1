#%Module1.0#####################################################################
##
## eigen modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tEigen - Adds Eigen 3.1.1 to your path"
}

module-whatis	"eigen-3.1.1"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    CPATH       /opt/eigen/3.1.1/include

module		use		/usr/local/Modules/versions

