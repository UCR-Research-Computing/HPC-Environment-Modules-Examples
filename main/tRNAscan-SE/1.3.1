#%Module1.0#####################################################################
##
## tRNAscan modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\ttRNAscan- Adds tRNAscan to your path"
}

module-whatis	"tRNAscan-SE-1.3.1"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PERL5LIB	/opt/tRNAscan-SE/1.3.1/bin
prepend-path	MANPATH	/opt/tRNAscan-SE/1.3.1/man
prepend-path	PATH	/opt/tRNAscan-SE/1.3.1/bin

module		use		/usr/local/Modules/versions
