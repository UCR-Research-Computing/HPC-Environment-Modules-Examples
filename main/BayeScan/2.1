#%Module1.0#####################################################################
##
## BayeScan modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tBayeScan - Adds BayeScan 2.1 to your path"
}

module-whatis   "BayeScan-2.1"

# for Tcl script use only
set     version         3.2.9
set     prefix          /usr/local/Modules/3.2.9
set     exec_prefix     ${prefix}
set     datarootdir     ${prefix}/share

module load proj
prepend-path     PATH              /opt/BayeScan/2.1/bin

module           use      /usr/local/Modules/versions
