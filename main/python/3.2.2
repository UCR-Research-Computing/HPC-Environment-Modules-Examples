#%Module1.0#####################################################################
##
## Python3 modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tpython3 - Adds python3 to your path"
}

module-whatis	"python-3.2.2"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		/opt/Python/3.2.2/bin
prepend-path    MANPATH  	/opt/Python/3.2.2/share/man

module		use		/usr/local/Modules/versions

