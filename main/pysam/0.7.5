#%Module1.0#####################################################################
##
## pysam modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tpysam - Adds pysam library to your python path"
}

module-whatis	"pysam-0.7.5"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path PYTHONPATH /opt/pysam/0.7.5/lib/python2.7/site-packages
module		use		/usr/local/Modules/versions
