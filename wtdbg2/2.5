#%Module1.0#####################################################################
##
## wtdbg2 modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t wtdbg2 - Adds wtdbg2 assembler to your path"
}

module-whatis	"wtdbg2-2.5"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH        ${hpcc_modules}/wtdbg2/2.5/bin
prepend-path    PATH        ${hpcc_modules}/wtdbg2/2.5/scripts

