#%Module1.0#####################################################################
##
## maui modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgnuplot - Adds gnuplot to your path"
}

module-whatis	"gnuplot-5.0.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH            ${hpcc_modules}/gnuplot/5.0.1/bin:${hpcc_modules}/gnuplot/5.0.1/libexec/gnuplot/5.0
prepend-path	MANPATH		${hpcc_modules}/gnuplot/5.0.1/share/man

module		use		${modules_dir}

