#%Module1.0#####################################################################
##
## maui modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\ttrinity-rnaseq - Adds trinity-rnaseq to your path"
}

module-whatis	"trinity-rnaseq-2.1.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH            ${hpcc_modules}/trinity-rnaseq/2.1.1
prepend-path    PATH            ${hpcc_modules}/trinity-rnaseq/2.1.1/util
prepend-path    CLASSPATH     	${hpcc_modules}/trinity-rnaseq/2.1.1/java
setenv		TRINITY_HOME	${hpcc_modules}/trinity-rnaseq/2.1.1

module		use		${modules_dir}

