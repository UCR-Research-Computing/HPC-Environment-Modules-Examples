#%Module1.0#####################################################################
##
## maui modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\ttrinity-rnaseq - Adds trinity-rnaseq to your path"
}

module-whatis	"trinity-rnaseq-2.0.6"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH            ${iigb_modules}/trinity-rnaseq/2.0.6
prepend-path    PATH            ${iigb_modules}/trinity-rnaseq/2.0.6/util
prepend-path    CLASSPATH     	${iigb_modules}/trinity-rnaseq/2.0.6/java
setenv		TRINITY_HOME	${iigb_modules}/trinity-rnaseq/2.0.6

module		use		${modules_dir}

