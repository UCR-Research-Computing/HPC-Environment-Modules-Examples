#%Module1.0#####################################################################
##
## maui modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tbeast - Adds beast to your path"
}

module-whatis	"beast-2.2.1"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		${iigb_modules}/beast/2.2.1/bin
prepend-path    CLASSPATH       ${iigb_modules}/beast/2.2.1/lib

module		use		${modules_dir}

