#%Module1.0#####################################################################
##
## beast modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tbeast - Adds beast to your path"
}

module-whatis	"beast-1.10.4"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH   		${hpcc_modules}/beast/1.10.4/bin
prepend-path    CLASSPATH   ${hpcc_modules}/beast/1.10.4/lib
setenv          BEAST_LIB   ${hpcc_modules}/beast/1.10.4/lib

