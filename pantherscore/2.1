#%Module1.0#####################################################################
##
## PantherScore modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tpantherscore - Adds pantherScore to your path"
}

module-whatis	"pantherscore-2.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH            ${hpcc_modules}/pantherscore/2.1
prepend-path    PERL5LIB        ${hpcc_modules}/pantherscore/2.1/lib


