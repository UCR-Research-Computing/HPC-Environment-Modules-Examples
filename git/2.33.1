#%Module1.0#####################################################################
##
## git modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgit - Adds git to your path"
}

module-whatis	"git-2.33.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    MANPATH    ${hpcc_modules}/git/share/man
prepend-path    PATH       ${hpcc_modules}/git/2.33.1/bin:${hpcc_modules}/git/2.33.1/libexec
prepend-path    PERL5LIB   ${hpcc_modules}/git/2.33.1/lib

