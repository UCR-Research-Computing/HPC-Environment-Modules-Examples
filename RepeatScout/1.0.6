#%Module1.0#####################################################################
##
## RepeatScout modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tRepeatScout- Adds RepeatScout to your path"
}

module-whatis	"RepeatScout-1.0.6"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share



prepend-path    PATH            ${hpcc_modules}/RepeatScout/1.0.6/bin


