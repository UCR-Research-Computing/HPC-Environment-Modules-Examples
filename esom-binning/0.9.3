#%Module1.0#####################################################################
##
## databionic-esom modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tesom-binning - Adds esom-binning scripts to your path"
}

module-whatis   "esom-binning-0.9.3"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set     version         3.2.9
set     prefix          ${modules_dir}/${version}
set     exec_prefix     ${prefix}
set     datarootdir     ${prefix}/share

prepend-path    PATH             ${hpcc_modules}/esom-binning/0.9.3
module           use              ${modules_dir}

