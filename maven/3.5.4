#%Module1.0#####################################################################
##
## Maven modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tMaven - Adds Maven to your path"
}

module-whatis	"Maven-3.5.4"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH                    ${iigb_modules}/maven/3.5.4/bin
#prepend-path    LD_LIBRARY_PATH         ${iigb_modules}/maven/3.5.4/lib
#prepend-path    LIBRARY_PATH            ${iigb_modules}/maven/3.5.4/lib

module		use		${modules_dir}

