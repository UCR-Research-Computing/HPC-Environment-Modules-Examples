#%Module1.0#####################################################################
##
## kat modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tkat - Adds kat 2.3.4 to your path"
}

module-whatis	"kat-2.3.4"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set     prefix          ${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prereq  python/3.6.0

prepend-path    PATH            ${iigb_modules}/kat/2.3.4/bin
prepend-path    CPATH           ${iigb_modules}/kat/2.3.4/include
prepend-path    LD_LIBRARY_PATH ${iigb_modules}/kat/2.3.4/lib
prepend-path    MANPATH         ${iigb_modules}/kat/2.3.4/share/man

module use ${modules_dir}
