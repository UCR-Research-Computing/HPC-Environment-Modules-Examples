#%Module1.0#####################################################################
##
## gocryptfs modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgocryptfs- Adds gocryptfs to your path"
}

module-whatis	"gocryptfs-1.7"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH            ${iigb_modules}/gocryptfs/1.7/bin
prepend-path    MANPATH         ${iigb_modules}/gocryptfs/1.7/share/man

