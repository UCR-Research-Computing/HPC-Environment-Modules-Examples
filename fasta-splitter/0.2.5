#%Module1.0#####################################################################
##
## fasta-splitter modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tfasta-splitter- Adds fasta-splitter to your path"
}

module-whatis	"fasta-splitter-0.2.5"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH              ${hpcc_modules}/fasta-splitter/0.2.5/

module		use		${modules_dir}

