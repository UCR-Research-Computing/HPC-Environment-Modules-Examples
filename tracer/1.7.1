#%Module1.0#####################################################################
##
## tracer modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\ttracer - Adds tracer to your path"
}

module-whatis	"tracer-1.7.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

module load java/13
module load beagle-lib
prepend-path    PATH        ${hpcc_modules}/tracer/1.7.1/bin
setenv          CLASSPATH	${hpcc_modules}/tracer/1.7.1/lib

