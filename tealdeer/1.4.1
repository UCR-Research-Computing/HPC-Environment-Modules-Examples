#%Module1.0#####################################################################
##
## tealdeer modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\ttealdeer- Adds tealdeer to your path"
}

module-whatis	"tealdeer-1.4.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH                ${hpcc_modules}/tealdeer/1.4.1/bin
setenv          TEALDEER_CONFIG_DIR ${hpcc_modules}/tealdeer/1.4.1/etc                                                                                                  
setenv          TEALDEER_CACHE_DIR  ${hpcc_modules}/tealdeer/1.4.1/cache

