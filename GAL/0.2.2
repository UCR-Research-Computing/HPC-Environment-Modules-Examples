#%Module1.0#####################################################################
##
## GAL modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tGAL- Adds GAL to your path"
}

module-whatis	"GAL-0.2.2"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH          ${hpcc_modules}/GAL/0.2.2/bin
prepend-path    PERL5LIB      ${hpcc_modules}/GAL/0.2.2/lib

module switch perl/5.20.2
