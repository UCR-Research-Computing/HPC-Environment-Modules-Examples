#%Module1.0#####################################################################
##
## chloroextractor modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tchloroextractor- Adds chloroextractor to your path"
}

module-whatis	"chloroextractor-1.0.1"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH              ${iigb_modules}/chloroextractor/1.0.5/

module		use		${modules_dir}

