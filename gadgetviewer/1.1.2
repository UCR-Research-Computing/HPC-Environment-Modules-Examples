#%Module1.0#####################################################################
##
## gadgetviewer modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgadgetviewer - Adds gadgetviewer 1.1.2 pdsh 2.29 to your path"
}

module-whatis   "gadgetviewer-1.1.2"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

prepend-path     PATH               ${hpcc_modules}/gadgetviewer/1.1.2/bin
prepend-path     LD_LIBRARY_PATH    ${hpcc_modules}/gadgetviewer/1.1.2/lib
prepend-path     MANPATH            ${hpcc_modules}/gadgetviewer/1.1.2/share

