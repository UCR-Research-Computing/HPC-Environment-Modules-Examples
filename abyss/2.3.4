#%Module1.0#####################################################################
##
## abyss modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tabyss - Adds ABYSS to your path"
}

module-whatis	"abyss-2.3.4"

set      hpcc_modules    $::env(HPCC_MODULES)

setenv          ABYSS_DIR   ${hpcc_modules}/abyss/2.3.4/

prepend-path	PATH	    ${hpcc_modules}/abyss/2.3.4/bin
prepend-path	MANPATH     ${hpcc_modules}/abyss/2.3.4/share/man

