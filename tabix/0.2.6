#%Module1.0#####################################################################
##
## tabix modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\ttabix - Adds tabix to your path"
}

module-whatis	"tabix-0.2.6"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH            ${iigb_modules}/tabix/0.2.6/bin
setenv          TABIX_SRC       ${iigb_modules}/tabix/0.2.6

module		use		${modules_dir}
