#%Module1.0#####################################################################
##
## maui modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tmapsembler2 - Adds mapsembler2 to your path"
}

module-whatis	"mapsembler2-2.2.4"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH            ${hpcc_modules}/mapsembler2/2.2.4/tools
prepend-path    PATH            ${hpcc_modules}/mapsembler2/2.2.4


