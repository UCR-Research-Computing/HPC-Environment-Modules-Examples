#%Module1.0#####################################################################
##
## GAEMR modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tGAEMR- Adds GAEMR assembly quality assessment tool to your path"
}

module-whatis	"GAEMR-1.0.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH          ${hpcc_modules}/GAEMR/1.0.1/bin
prepend-path    PYTHONPATH    ${hpcc_modules}/GAEMR/1.0.1

