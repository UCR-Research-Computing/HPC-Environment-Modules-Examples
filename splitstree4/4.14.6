#%Module1.0#####################################################################
##
## splitstree4 modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tsplitstree4 - Adds splitstree4 to your path"
}

module-whatis	"splitstree4-4.14.6"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH    ${hpcc_modules}/splitstree4/4.14.6

