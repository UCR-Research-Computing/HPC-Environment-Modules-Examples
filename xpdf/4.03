#%Module1.0#####################################################################
##
## xpdf modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\txpdf- Adds xpdf to your path"
}

module-whatis	"xpdf-4.03"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/xpdf/4.03/bin
prepend-path    MANPATH         ${hpcc_modules}/xpdf/4.03/share/man

