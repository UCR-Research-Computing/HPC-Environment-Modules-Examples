#%Module1.0#####################################################################
##
## EIG modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\teigenstrat-hawk - eigenstrat-hawk to your library path runpath"
}

module-whatis	"eigenstrat-hawk-6.0.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	${hpcc_modules}/eigenstrat-hawk/6.0.1/bin
module		use		${modules_dir}

