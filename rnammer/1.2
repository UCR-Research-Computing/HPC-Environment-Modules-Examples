#%Module1.0#####################################################################
##
## RNAmmer modulefile
##
proc ModulesHelp { } {
        global version prefix

       puts stderr "\tMEME - Adds RNAmmer to your path"
}

module-whatis	"RNAmmer-1.2"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	${hpcc_modules}/rnammer/1.2
prepend-path	MANPATH	${hpcc_modules}/rnammer/1.2/man
