#%Module1.0#####################################################################
##
## unimap modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tunimap- Adds unimap to your path"
}

module-whatis	"unimap-0.1"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/unimap/0.1/bin
prepend-path    MANPATH         ${hpcc_modules}/unimap/0.1/share/man

