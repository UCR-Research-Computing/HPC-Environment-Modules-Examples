#%Module1.0#####################################################################
##
## maui modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tmasurca - Adds mascura to your path"
}

module-whatis	"masurca-2.3.2"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH            ${iigb_modules}/masurca/2.3.2/bin
prepend-path	CPATH		${iigb_modules}/masurca/2.3.2/include
prepend-path 	LIBRARY_PATH	${iigb_modules}/masurca/2.3.2/lib
prepend-path    MANPATH         ${iigb_modules}/masurca/2.3.2/share/man

module		use		${modules_dir}

