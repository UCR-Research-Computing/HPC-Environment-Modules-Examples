#%Module1.0#####################################################################
##
## geos modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgeos- Adds geos to your path"
}

module-whatis	"geos-3.7.1"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH              ${iigb_modules}/geos/3.7.1/bin
prepend-path    LD_LIBRARY_PATH   ${iigb_modules}/geos/3.7.1/lib
prepend-path    INCLUDE_PATH      ${iigb_modules}/geos/3.7.1/include

