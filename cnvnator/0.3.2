#%Module1.0#####################################################################
##
## CNVnator modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tCNVnator - Adds CNVnator seq perl scripts to your path"
}
module-whatis   "CNVnator-0.3.2"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set     version         3.2.9
set     prefix          ${modules_dir}/${version}
set     exec_prefix     ${prefix}
set     datarootdir     ${prefix}/share

prepend-path    PATH             ${hpcc_modules}/cnvnator/0.3.2/bin
module load yeppp
module load root

