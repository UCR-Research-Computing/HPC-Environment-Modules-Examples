#%Module1.0#####################################################################
##
## cnvnator modulefile
#
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tcnvnator - Adds cnvnator and cnvpytor suite of tools to your path"
}

module-whatis	"cnvnator-0.4.1-cnvpytor-1.2.1"

set      hpcc_modules    $::env(HPCC_MODULES)

if { [module-info mode load] || [module-info mode switch2] } {
    puts stdout "module load miniconda3; conda activate ${hpcc_modules}/cnvnator/0.4.1;"
} elseif { [module-info mode remove] && ![module-info mode switch3] } {
    puts stdout "conda deactivate;"
}
