#%Module1.0#####################################################################
##
## adapterremoval modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tadaptorremoval - Adds AdapterRemoval to your path"
}

module-whatis	"adapterremoval-2.2.2"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH            ${hpcc_modules}/adapterremoval/2.2.2/bin
prepend-path	MANPATH		${hpcc_modules}/adapterremoval/2.2.2/share/man

