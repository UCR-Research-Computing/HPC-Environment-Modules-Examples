#%Module1.0#####################################################################
##
## VarScan modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tvarscan - Adds VarScan jar file variable to your environment"
}

module-whatis	"varscan-2.3.9"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH            ${hpcc_modules}/varscan/2.3.9/scripts
setenv		VARSCANHOME	${hpcc_modules}/2.3.9
setenv		VARSCANJAR	${hpcc_modules}/2.3.9/VarScan.jar
module		use		${modules_dir}
