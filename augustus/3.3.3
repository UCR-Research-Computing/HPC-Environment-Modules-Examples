#%Module1.0#####################################################################
##
## augustus modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\taugustus - Adds AUGUSTUS to your path"
}

module-whatis	"augustus-3.3.3"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH                    ${hpcc_modules}/augustus/3.3.3/bin:${hpcc_modules}/augustus/3.3.3/scripts
setenv          AUGUSTUS_CONFIG_PATH    ${hpcc_modules}/augustus/3.3.3/config

