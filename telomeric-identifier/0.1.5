#%Module1.0#####################################################################
##
## tidk modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\ttelomeric-identifier - Adds tidk to your path"
}

module-whatis	"telomeric-identifier-0.1.5"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH            ${hpcc_modules}/telomeric-identifier/0.1.5/


