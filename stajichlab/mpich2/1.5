#%Module1.0#####################################################################
##
## ipe modulefile
##
proc ModulesHelp { } {
        global version prefix

       puts stderr "\tmpich- Adds mpicc to your path"
}

module-whatis	"mpich2-1.5"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/stajichlab/mpich2/1.5/bin
prepend-path	LD_LIBRARY_PATH	/opt/stajichlab/mpich2/1.5/lib
prepend-path	MANPATH	/opt/stajichlab/mpich2/1.5/share/man
module		use		/usr/local/Modules/versions
