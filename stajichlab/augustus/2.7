#%Module1.0#####################################################################
##
## augustus modulefile
##
proc ModulesHelp { } {
        global version prefix

       puts stderr "\taugustus- Adds augustus to your path"
}

module-whatis	"augustus-2.7"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/stajichlab/augustus/2.7/bin
prepend-path	PATH	/opt/stajichlab/augustus/2.7/scripts
setenv	AUGUSTUS_CONFIG_PATH	/opt/stajichlab/augustus/2.7/config
module		use		/usr/local/Modules/versions
