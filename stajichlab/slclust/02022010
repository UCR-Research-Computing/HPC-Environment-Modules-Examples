#%Module1.0#####################################################################
##
## slclust modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tslclust Adds slclust to your path"
}

module-whatis	"slclust 02022010"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/stajichlab/slclust/02022010/bin

module		use		/usr/local/Modules/versions
