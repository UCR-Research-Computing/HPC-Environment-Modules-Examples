#%Module1.0#####################################################################
##
## quake modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tquake - Adds quake to your path"
}

module-whatis	"quake-0.3.0"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/stajichlab/quake/0.3.0/bin

module		use		/usr/local/Modules/versions
