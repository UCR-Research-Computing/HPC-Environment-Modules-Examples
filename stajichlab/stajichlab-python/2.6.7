#%Module1.0#####################################################################
##
## stajichlab-pytohon modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tstajichlab-python- Adds Stajich Lab python to your path"
}

module-whatis	"stajichlab-python 2.6.7"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/stajichlab/stajichlab-python/2.6.7/bin
prepend-path	LD_LIBRARY_PATH	/opt/stajichlab/stajichlab-python/2.6.7/lib
prepend-path	PYTHONPATH	/opt/stajichlab/stajichlab-python/2.6.7/lib/python2.7/site-packages
prepend-path	MANPATH		/opt/stajichlab/stajichlab-python/2.6.7/share/man

module		use		/usr/local/Modules/versions
