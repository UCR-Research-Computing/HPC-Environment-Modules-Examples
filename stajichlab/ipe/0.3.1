proc ModulesHelp { } {
        global version prefix

       puts stderr "\tiParameterEstimation - Adds ipe to your path"
}

module-whatis	"ipe 0.3.1 for gene prediction training"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
