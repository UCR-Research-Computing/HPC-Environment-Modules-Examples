prepend-path	PATH	/opt/stajichlab/ipe/0.3.1/bin
prepend-path	PERL5LIB	/opt/stajichlab/ipe/0.3.1/lib/perl5
prepend-path	MANPATH	/opt/stajichlab/ipe/0.3.1/man
module		use		/usr/local/Modules/versions
