proc ModulesHelp { } {
        global version prefix

        puts stderr "\tmaven - Adds maven software your path"
}

module-whatis	"maven-3.0.4"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/stajichlab/maven/3.0.4/bin
setenv	M2_HOME	/opt/stajichlab/maven/3.0.4
module		use		/usr/local/Modules/versions
