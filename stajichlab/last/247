#%Module1.0#####################################################################
##
## last modulefile
##
proc ModulesHelp { } {
        global version prefix

       puts stderr "\tlast - Adds last and associated tools to your path"
}

module-whatis	"last-247"

# for Tcl script use only
set	version		3.2.9
set	prefix		/usr/local/Modules/3.2.9
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	/opt/stajichlab/last/247/bin
module		use		/usr/local/Modules/versions
