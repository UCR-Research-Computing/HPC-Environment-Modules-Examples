#%Module1.0#####################################################################
##
## pcap/cap modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tcap3 - Adds cap3 to your path"
}

module-whatis	"cap3-2015.02.10"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/cap3/2015-02-10

