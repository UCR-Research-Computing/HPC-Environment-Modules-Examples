#%Module1.0#####################################################################
##
## mira-assembler modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tmira-assembler - Adds mira-assembler to your path"
}

module-whatis	"mira-assembler-5rc2"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/mira-assembler/5rc2/bin
