#%Module1.0#####################################################################
##
## PartitionFinder modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tPartitionFinder - Adds PartitionFinder.py and PartitionFinderProtein.py to your path"
}

module-whatis	"PartitionFinder-2.1.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prereq  python/2.7.12

prepend-path    PATH    ${hpcc_modules}/PartitionFinder/2.1.1

module use ${modules_dir}
