#%Module1.0#####################################################################
##
## OligoArray 2.1 modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tOligoArray 2.1- Adds OligoArray 2.1to your path"
}

module-whatis	"OligoArray-2.1"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH            ${iigb_modules}/oligoarray/2.1/
module		use		${modules_dir}
setenv          OLIGOARRAYDIR         ${iigb_modules}/oligoarray/2.1/

module load oligoarrayaux
module load ncbi-blast/2.2.26
module load java/6
