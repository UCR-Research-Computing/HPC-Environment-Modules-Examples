#%Module1.0#####################################################################
##
## pandoc modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tpandoc - Adds pandoc to your path"
}

module-whatis	"pandoc-2.9.2.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

module load texlive

prepend-path    PATH            ${hpcc_modules}/pandoc/2.9.2.1/bin


