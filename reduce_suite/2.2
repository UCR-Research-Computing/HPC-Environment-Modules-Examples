#%Module1.0#####################################################################
##
## reduce_suite modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\treduce_suite - Adds reduce_suite 2.2 to your path"
}

module-whatis   "reduce_suite-2.2"

set      hpcc_modules    $::env(HPCC_MODULES)

setenv          reduce_SUITE   ${hpcc_modules}/reduce_suite/2.2
prepend-path    PATH           ${hpcc_modules}/reduce_suite/2.2/bin

