#%Module1.0#####################################################################
##
## newick-utils modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tnewick-utils - Adds newick-utils (nw_*) to your path"
}

module-whatis	"newick-utils-1.7"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH            ${hpcc_modules}/newick-utils/1.7/bin
prepend-path    LD_LIBRARY_PATH ${hpcc_modules}/newick-utils/1.7/lib

module use ${modules_dir}
