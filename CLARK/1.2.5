#%Module1.0#####################################################################
##
## CLARK modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tCLARK- Adds CLARK to your path"
}

module-whatis	"CLARK-1.2.5"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH              ${hpcc_modules}/CLARK/1.2.5/exe:${hpcc_modules}/CLARK/1.2.5

