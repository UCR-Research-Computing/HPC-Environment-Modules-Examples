#%Module1.0#####################################################################
##
## kubernetes modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tkubernetes- Adds kubectl to your path"
}

module-whatis	"kubernetes-1.19.4"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path    PATH            ${hpcc_modules}/kubernetes/1.19.4

