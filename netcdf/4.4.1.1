#%Module1.0#####################################################################
##
## netcdf modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tnetcdf- Adds netcdf to your path"
}

module-whatis	"netcdf-4.4.1.1"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH              ${iigb_modules}/netcdf/4.4.1.1/bin
prepend-path    LD_LIBRARY_PATH   ${iigb_modules}/netcdf/4.4.1.1/lib
prepend-path	MANPATH           ${iigb_modules}/netcdf/4.4.1.1/share/man

module		use		${modules_dir}

