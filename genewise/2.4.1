#%Module1.0#####################################################################
##
## maui modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgenewise - Adds genewise to your path"
}

module-whatis	"genewise-2.4.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		${hpcc_modules}/genewise/2.4.1/bin
setenv WISECONFIGDIR	${hpcc_modules}/genewise/2.4.1/wisecfg

module		use		${modules_dir}

