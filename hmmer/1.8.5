#%Module1.0#####################################################################
##
## maui modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\thmmer - Adds hmmer1 apps to your path"
}

module-whatis	"hmmer-1.8.5"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH            ${iigb_modules}/hmmer/1.8.5/bin
prepend-path    MANPATH         ${iigb_modules}/hmmer/1.8.5/man

module		use		${modules_dir}

